<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background_main"
    android:layoutDirection="rtl"
    tools:context="CardNotPresent.Home.ui.Home.HomeFragment">

    <!-- 1 -->
    <LinearLayout
        android:id="@+id/frag_home_inner_1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="visible">

        <include layout="@layout/content_frag_home_inner_1" />
    </LinearLayout>

    <!-- 2 -->
    <LinearLayout
        android:id="@+id/frag_home_inner_2"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone">

        <include layout="@layout/content_frag_home_inner_2" />
    </LinearLayout>

    <!-- 3 -->
    <LinearLayout
        android:id="@+id/frag_home_inner_3"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone">

        <include layout="@layout/content_frag_home_inner_3" />
    </LinearLayout>

    <!-- 4 -->
    <LinearLayout
        android:id="@+id/frag_home_inner_4"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone">

        <include layout="@layout/content_frag_home_inner_4" />
    </LinearLayout>

    <!-- 5 -->
    <LinearLayout
        android:id="@+id/frag_home_inner_5"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone">

        <include layout="@layout/content_frag_home_inner_5" />
    </LinearLayout>

    <!-- 6 -->
    <LinearLayout
        android:id="@+id/frag_home_inner_6"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone">

        <include layout="@layout/content_frag_home_inner_6" />
    </LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>